Bike Race
#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d,e,f,g;
    cin>>a>>b>>c>>d;
    e=a%d;
    f=b%d;
    g=c%d;
    if(e==0)
    {
        cout<<"Bike 1 goes into road A";
    }
    else if(f==0)
    {
        cout<<"Bike 1 goes into road B";
    }
    else if(g==0)
    {
        cout<<"Bike 1 goes into road C";
    }
    else
    {
        cout<<"No path exist";
    }
    return 0;
}
