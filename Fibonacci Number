Fibonacci Number
#include<iostream>
using namespace std;
int main()
{
   int a,b=1,c=1,d,i;
   cin>>a;
   for(i=1;i<=a;i++)
   {
       d=b+c;
       b=c;
       c=d;
   }
   if(b==a)
   {
           cout<<"Fibonacci number";
    }
    else
    {
           cout<<"Not Fibonacci number";
    }
   
}
